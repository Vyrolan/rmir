[General]
Name=Kenwood R720 Remote
EepromSize=$400
AdvCodeAddr=$029..$0FF
UpgradeAddr=$100..$3FC
Processor=S3C80
RAMAddr=$8000
RDFSync=3
ImageMap=Kenwood R720.map

[SpecialProtocols]
Multiplex=01FE
Pause=01FB (Pause)

[Settings]
VPT Device=$01B.7.8.1.0 DeviceButtons
VPT Status=$01C.3.1.0.0 (Off;On)

[Checksums]
^$000:$002..$3FC

[DeviceTypes]
Cable = 0
TV    = 0
DVD   = 1
VCR   = 2
Audio = 3
CD    = 4

[DeviceTypeAliases]
Cable   = Cable,SAT,Video Acc
TV      = TV
DVD	= DVD,Laserdisc
VCR     = VCR,Tape,DAT
Audio   = Tuner,Misc Audio,Amp
CD	= CD,Home Auto,Phono

[DeviceButtons]
dev0 =	$00A $00B
dev1 =	$00C $00D
dev2 =	$00E $00F
dev3 =	$010 $011
dev4 =	$012 $013
dev5 =	$014 $015
dev6 =	$016 $017

